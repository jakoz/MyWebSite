@model MyWebSite.ViewModels.ScheduleTaskViewModel

@foreach (var schedule in Model.Schedules)
{
    <div class="col-md-offset-1 col-md-5 calendar">
        <div class="calendar-header">
        </div>
        <div class="calendar-body" id="@schedule.DateOfSchedule.ToShortDateString()">
            @schedule.DateOfSchedule.ToShortDateString()
        </div>        
    </div>    
}
